{
    "name": "kilroy.ui_agent_template/manager/new_viewer_agent",
    "version": "1",
    "description": "",
    "isInteractive": true,
    "clean_up": true,
    "inputs": [],
    "outputs": [
        "agent_.*",
        "ex_.*"
    ],
    "args": {
        "role": "*"
    },
    "steps": [
