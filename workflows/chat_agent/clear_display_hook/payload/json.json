{
    "name": "kilroy.ui_agent_template/chat_agent/clear_display_hook",
    "version": "1",
    "description": "",
    "isInteractive": false,
    "clean_up": true,
    "inputs": [],
    "outputs": [],
    "args": {
        "role": "*"
    },
    "steps": [
        {
            "operation": "set_value",
            "isServer": true,
            "args": {
                "variable": "wfCleanupFlag",
                "value": "1"
            }
        },
        {
            "operation": "step_group",
            "isServer": true,
            "args": {
                "role": "*",
                "message": "",
                "timer": "24",
                "isInteractive": false,
                "canTerminate": false,
                "success": [
                    {
                        "operation": "set_var_to",
                        "isServer": true,
                        "args": {
                            "var_name": "html",
                            "push_array_name": null,
                            "field_name": null,
                            "var_expr": {
                                "type": "object",
                                "arg": "webhook_args",
                                "field_name": "output_template"
                            }
                        }
                    },
                    {
                        "operation": "if_then_else",
                        "isServer": true,
                        "args": {
                            "condition": "\"${html}\" == \"undefined\"",
                            "success": [
                                {
                                    "operation": "set_var_to",
                                    "isServer": true,
                                    "args": {
                                        "var_name": "html",
                                        "push_array_name": null,
                                        "field_name": null,
                                        "var_expr": {
                                            "type": "value",
                                            "arg": ""
                                        }
                                    }
                                }
                            ],
                            "failure": []
                        }
                    },
                    {
                        "operation": "set_var_to",
                        "isServer": true,
                        "args": {
                            "var_name": "args",
                            "push_array_name": null,
                            "field_name": null,
                            "var_expr": {
                                "type": "value",
                                "arg": {}
                            }
                        }
                    },
                    {
                        "operation": "set_var_to",
                        "isServer": true,
                        "args": {
                            "var_name": "args",
                            "push_array_name": null,
                            "field_name": "html",
                            "var_expr": {
                                "type": "variable",
                                "arg": "html"
                            }
                        }
                    },
                    {
                        "operation": "send_pubsub_cmd",
                        "isServer": true,
                        "args": {
                            "command": "UPDATE_PUBSUB_AREA",
                            "args": "${args}",
                            "topic": "${pd_alias}"
                        }
                    }
                ]
            }
        },
        {
            "operation": "end",
            "isServer": true,
            "args": null
        }
    ],
    "externalForms": {}
}